{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "allowImportingTsExtensions": false,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": "..",
        "paths": {
            "@/*": [
                "./*"
            ]
        }
    },
    "include": [
        "**/*.ts"
    ]
}